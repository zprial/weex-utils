{
  "name": "weex-utils",
  "version": "0.1.1",
  "description": "Utils for smooth weex and web platform gap",
  "author": "zprial",
  "private": false,
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean && npm run build:prod && npm run build:index",
    "build:prod": "NODE_ENV=production webpack --env.NODE_ENV=production && echo ok",
    "build:index": "babel src/index.js  -o lib/index.js",
    "clean": "rimraf ./lib",
    "lint": "eslint --ext .js,.vue src  --fix",
    "serve": "webpack-dev-server --env.NODE_ENV=development --progress && echo ok",
    "analyse": "npm run clean && NODE_ENV=analyse webpack --env.NODE_ENV=production && echo ok"
  },
  "keywords": [
    "weex"
  ],
  "main": "lib/index.js",
  "files": [
    "src",
    "lib/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zprial/weex-utils.git"
  },
  "license": "MIT",
  "dependencies": {
    "querystringify": "^2.0.0",
    "requires-port": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.9.2",
    "portfinder": "^1.0.16",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-plugin-weex": "^0.1.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "url-loader": "^1.0.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.3.0",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "weex-loader": "^0.7.9",
    "weex-utils": "^0.1.1",
    "weex-vue-precompiler": "^0.1.22",
    "weex-vue-render": "^1.0.28"
  }
}
